{
    "name": "yakoffka/laravel-notification-channels-ru-store",
    "description": "RuStore push notifications channel for Laravel",
    "homepage": "https://github.com/yakoffka/laravel-notification-channels-ru-store",
    "license": "MIT",
    "authors": [
        {
            "name": "yakOffKa",
            "email": "yagithub@mail.ru",
            "homepage": "https://yakoffka.ru/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/notifications": "~10.0 || ~11.0",
        "illuminate/support": "~10.0 || ~11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\RuStore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\RuStore\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\RuStore\\RuStoreServiceProvider"
            ]
        }
    }
}
